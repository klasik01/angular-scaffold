{
  "name": "angular-scaffold",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "start:mock": "concurrently \"ng serve --configuration mock\" \"npm run watch-jsonserver\"",
    "dev:up": "docker-compose -f docker-compose.yml up --build ng-serve-local",
    "prod:up": "docker-compose -f docker-compose.yml up --build app-prod-oidc",
    "dev:down": "docker-compose -f docker-compose.yml down",
    "build": "npm run build:tag && npm run build:code",
    "build:analyze": "npm run build:code:report && npm run build:analyze:html",
    "build:analyze:html": "webpack-bundle-analyzer dist/app-angular/stats.json -m static -r reports/sourcemap.html -O",
    "build:code:report": "npm run build:code -- --stats-json",
    "build:code": "ng build --configuration production",
    "build:tag": "node ./scripts/tagBuildVersion.js",
    "clean": "rm -rf reports && rm -rf coverage",
    "commit:lint": "commitlint",
    "release": "standard-version",
    "prettier:check": "./node_modules/.bin/prettier \"**/*.ts\" --check ",
    "prettier:write": "./node_modules/.bin/prettier \"**/*.ts\" --write",
    "test": "npm run test:lint && npm run test:unit:ci",
    "test:unit": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng test --browsers ChromeNoGpu --source-map=false --watch",
    "test:unit:browserless": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng test --browsers CH --source-map=false --no-progress --watch=true",
    "test:unit:coverage": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng test --code-coverage --browsers CH --source-map=false --watch",
    "test:unit:ci": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test --no-watch --code-coverage --browsers ChromeHeadlessCI --source-map=false --watch false",
    "test:unit:open:coverage": "open coverage/index.html",
    "test:lint": "ng lint",
    "test:vulnerabilities": "npm audit --json --registry=https://registry.npmjs.org| npm-audit-html --output reports/vulnerabilities.html",
    "test:e2e": "scripts/e2e.sh",
    "test:e2e:run": "cypress run --headless",
    "test:e2e:debug": "cypress open",
    "test:e2e:clean": "rm -fr e2e/reports",
    "test:e2e:run:cypress": "npx cypress run --config baseUrl=http://localhost:4300 --env LOCAL_HOST=http://localhost:4300 --browser chrome",
    "test:e2e:run:mock": "concurrently --success first  --kill-others \"npm:watch-test:e2e:run:mock\"  \"npm:watch-mock-build\" \"npm:watch-jsonserver\"  ",
    "test:e2e:report:generate_cucumber_html_report": "node e2e/cucumber-report.js",
    "watch-test:e2e:run:mock": "npx wait-on http-get://localhost:4300 && npm run test:e2e:run:cypress",
    "watch-jsonserver": "npx node  json-server/server.js",
    "watch-mock-build": "ng build --configuration  mock ",
    "postwatch-mock-build": "npm run watch-httpserver",
    "watch-httpserver": "angular-http-server --path ./dist/app-angular -p 4300 --silent",
    "prereports": "mkdir -p reports || true",
    "reports": "npm run prereports && npm run test:vulnerabilities && npm run test:licenses && npm run test:licenses:summary && npm run build:analyze",
    "reports:open": "open docs/index.html",
    "lint": "ng lint",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@larscom/ngx-translate-module-loader": "^3.0.5",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/entity": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/router-store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "@ngx-loading-bar/core": "^6.0.0",
    "@ngx-loading-bar/http-client": "^6.0.0",
    "@ngx-loading-bar/router": "^6.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "concurrently": "^7.0.0",
    "date-fns": "^2.28.0",
    "immer": "^9.0.7",
    "lodash.clonedeep": "^4.5.0",
    "material-icons": "^1.10.5",
    "messageformat": "^2.3.0",
    "ngx-matomo": "^0.1.4",
    "ngx-translate-messageformat-compiler": "^4.11.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/language-service": "^13.0.1",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-angular": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@cypress/webpack-preprocessor": "^5.11.0",
    "@ngrx/schematics": "^13.0.2",
    "@types/chai": "^4.3.0",
    "@types/cucumber": "^7.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "^27.4.0",
    "@types/json-server": "^0.14.4",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "angular-http-server": "1.10.0",
    "cypress": "^9.2.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-promise": "^1.1.0",
    "eslint": "^8.6.0",
    "husky": "^7.0.0",
    "jasmine-core": "~3.10.0",
    "jasmine-marbles": "^0.8.4",
    "jasmine-spec-reporter": "^7.0.0",
    "json-server": "^0.17.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "multiple-cucumber-html-reporter": "^1.18.3",
    "nodemon": "^2.0.15",
    "npm-audit-report": "^2.1.5",
    "prettier": "^2.5.1",
    "rxjs-marbles": "^7.0.1",
    "standard-version": "^9.3.2",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.5.2",
    "wait-on": "^6.0.0",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "engines": {
    "node": ">= 11.0.0",
    "npm": ">= 6.2.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "commonPath": "e2e/features/common",
    "stepDefinitions": "e2e/features/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "e2e/reports/cucumber",
      "filePrefix": "",
      "fileSuffix": ""
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose",
      "pre-commit": "npm run test:lint && npm run prettier:check"
    }
  }
}
