{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "../dist/out-tsc/e2e",
    "module": "esnext",
    "esModuleInterop": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "./**/*.ts",
    "./cypress/support/index.d.ts"
  ]
}
